{"legislative_session": "2026", "identifier": "HB 1401", "title": "State Lotteries", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["executive branch"], "abstracts": [{"note": "summary", "abstract": "State Lotteries;  Revises information required to be provided to Department of Lottery by persons who submit bid, proposal, or offer to negotiate contract for major procurement; revises bond amount retailer may be required to post for period within which retailer is required to remit lottery funds to department; authorizes division's sworn law enforcement officers to purchase & present lottery tickets to lottery retailer to claim prize; revises certain prohibitions & penalties relating to presenting counterfeit or altered state lottery ticket; prohibits certain false claims relating to state lottery tickets; prohibits lottery retailer from using such position to knowingly facilitate, participate in, or otherwise assist in theft of lottery ticket from retail establishment, patron, or customer; provides criminal penalties."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2026-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Persons-Mulicka", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Persons-Mulicka\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "H 1401 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/1401/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "24.103", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/24.103"}, {"publication": "Florida Statues", "citation": "24.105", "citation_type": "propos
{"legislative_session": "2026", "identifier": "HB 971", "title": "Family Law", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["domestic relations", "subject index"], "abstracts": [{"note": "summary", "abstract": "Family Law;  Requires that at least one judge be available in each judicial circuit on weekends, holidays, & after hours on weekdays to hear motions to enforce certain orders & agreements; requires chief judge to assign circuit judge to be available for certain hearings; requires that certain time-sharing matters be accorded priority on court's calendar; provides procedural requirements for evidentiary hearings on pleadings seeking temporary parental responsibility & time-sharing schedules & on motions to enforce compliance with existing time-sharing orders or agreements; requires court to issue upon motion by party, rather than authorizing court to make determination of, appropriate parenting plans."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2026-01-05", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Trabulsy", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Trabulsy\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "H 971 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/971/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "26.20", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/26.20"}, {"publication": "Florida Statues", "citation": "61.13", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/61.13"}, {"publication": "Florida Statue
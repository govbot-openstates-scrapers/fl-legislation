{"legislative_session": "2026", "identifier": "HB 767", "title": "Residential Property Insurance", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["subject index"], "abstracts": [{"note": "summary", "abstract": "Residential Property Insurance;  Requires that certain rate filings with OIR from residential property insurers include rate transparency reports; provides for office to accept such reports or to request that insurer make modifications; requires insurers to provide such reports to consumers; requires that report indicate that it is preliminary & subject to modification by insurer at direction of office under specified circumstance; requires office to define terms used in such reports; requires office to establish & maintain comprehensive resource center on its website; prohibits insurer from including value of land when establishing homeowner's policy coverage amount or adjusting certain claims."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2025-12-15", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Benarroch", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Benarroch\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "H 767 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/767/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "627.0621", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/627.0621"}, {"publication": "Florida Statues", "citation": "627.7011", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/627.7011"}, {"publication": "Florida
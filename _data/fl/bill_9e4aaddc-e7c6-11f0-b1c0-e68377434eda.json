{"legislative_session": "2026", "identifier": "HB 883", "title": "Protected Cell Captive Insurance Companies", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["subject index"], "abstracts": [{"note": "summary", "abstract": "Protected Cell Captive Insurance Companies;  Revises definition of \"captive insurance company\" to include protected cell captive insurance company; restricts coverage by protected cell captive insurance companies to protected cell participants; provides minimum capital, net assets & surplus requirements for protected cell captive insurance companies; provides requirements for formations of protected cell captive insurance companies; requires assets & liabilities of protected cells to be separate & separately identifiable from general accounts of captive insurance companies & from other protected cells; authorizes assets of different protected cells to be combined for specified purposes; provides procedures for pleadings in legal actions; provides procedures for protected cells for conversion to other forms, disaffiliation from captive insurance companies, & for merging with other protected cells."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2025-12-23", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Fabricio", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Fabricio\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "H 883 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/883/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "628.901", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/628.
{"legislative_session": "2026", "identifier": "SB 888", "title": "Professional Services Contracts", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["general assignments"], "abstracts": [{"note": "summary", "abstract": "Professional Services Contracts;  Providing that a professional services contract between a design professional and a contracting party, rather than between a design professional and a public agency, may require the design professional to indemnify and hold harmless the contracting party, and its officers and employees, only against certain liability and damages; specifying that a professional services contract must require a design professional to perform to a certain level of professional skill and care; prohibiting a professional services contract from subjecting a design professional to a different standard of care, etc."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2025-12-12", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Martin", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Martin\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "S 888 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/888/BillText/Filed/HTML", "media_type": "text/html"}, {"url": "https://flsenate.gov/Session/Bill/2026/888/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "725.08", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/725.08"}], "sources": [{"url": "https://flsenate.gov/Session/Bill/2026/888/ByCategory", "note": ""}], "extras": {}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:fl/government", "
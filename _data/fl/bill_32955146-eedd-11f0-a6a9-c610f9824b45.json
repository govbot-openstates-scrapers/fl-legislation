{"legislative_session": "2026", "identifier": "HB 1333", "title": "Testing of Cosmetics on Animals", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["subject index"], "abstracts": [{"note": "summary", "abstract": "Testing of Cosmetics on Animals;  Provides that it is unlawful for person to manufacture, repackage, sell, hold, or offer for sale cosmetics that have been tested on animals as part of manufacturing process; prohibits manufacturers from using animal testing as part of cosmetics manufacturing process; prohibits manufacturers from repackaging, selling, holding, or offering for sale cosmetics that have been tested on animals; provides that manufacturers, upon issuance of cosmetic manufacturer permit, consent to specified inspections by DBPR & to submit to DBPR certain written documentation by specified date each year; provides for criminal penalties; provides administrative penalty; provides for cause of action by DBPR."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2026-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Driskell", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Driskell\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "H 1333 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/1333/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "499.003", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/499.003"}, {"publication": "Florida Statues", "citation": "499.005", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/499.005"}, {"pu
{"legislative_session": "2026", "identifier": "HB 815", "title": "Roofing Requirements for Property Insurance", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["subject index"], "abstracts": [{"note": "summary", "abstract": "Roofing Requirements for Property Insurance;  Prohibits insurer from refusing to issue or renew property insurance policy, rather than homeowner's policy, insuring residential structure that has a roof less than specified age solely because the roof's age; requires insurer to differentiate between low-slope & steep-slope roofs in its offer for coverage under certain circumstances; requires insurer to allow property owner to have roof inspection performed on steep-slope roof by authorized inspector before requiring that roof be replaced; prohibits insurer from refusing to issue or renew property insurance policy under certain circumstances."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed", "date": "2025-12-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}], "sponsorships": [{"name": "Gottlieb", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Gottlieb\"}", "organization_id": null}, {"name": "Daley", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Daley\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "H 815 Filed", "links": [{"url": "https://flsenate.gov/Session/Bill/2026/815/BillText/Filed/PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "Florida Statues", "citation": "627.7011", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://flsenate.gov/laws/statutes/2025/627.7011"}], "sources": [{"url": "https://flsenate.gov/Session/Bill/2026/815/ByCategory", "note": ""}], "extras"